{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel start src/index.html --open",
    "build": "rimraf dist && parcel build src/index.html --no-cache --no-source-maps --detailed-report"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/coderslt/frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/coderslt/frontend#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "devDependencies": {
    "acorn": "^6.1.1",
    "babel-eslint": "^10.0.1",
    "csslint": "^1.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.0.0",
    "lint-staged": "^8.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
