{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel start src/index.html --open",
    "build": "rimraf dist && parcel build src/index.html --no-cache --no-source-maps --detailed-report"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/coderslt/frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/coderslt/frontend#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "devDependencies": {
    "acorn": "6.0.4",
    "babel-eslint": "10.0.1",
    "csslint": "1.0.5",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.1.3",
    "lint-staged": "8.0.4",
    "parcel-bundler": "1.10.3",
    "prettier": "1.15.2",
    "rimraf": "2.6.2",
    "stylelint": "9.8.0",
    "stylelint-config-recommended": "2.1.0"
  },
  "dependencies": {
    "react": "16.6.1",
    "react-dom": "16.6.1"
  }
}
