{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:dev": "rimraf .cache dist && parcel src/index.html --open",
    "start:server": "json-server data/demo.json --port 4321 --watch",
    "build": "rimraf dist && parcel build src/index.html --no-cache --no-source-maps --detailed-report",
    "precommit": "lint-staged",
    "postinstall": "flow-typed install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/coderslt/frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/coderslt/frontend#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-transform-react-jsx": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "acorn": "6.0.4",
    "babel-eslint": "10.0.1",
    "babel-plugin-flow-react-proptypes": "24.1.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "concurrently": "4.0.1",
    "css-module-flow": "1.0.0",
    "csslint": "1.0.5",
    "empty": "0.10.1",
    "enzyme": "3.7.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.3",
    "flow-bin": "0.89.0",
    "flow-typed": "2.5.1",
    "husky": "1.1.3",
    "json-server": "0.14.0",
    "lint-staged": "8.0.4",
    "parcel-bundler": "1.10.3",
    "prettier": "1.15.2",
    "rimraf": "2.6.2",
    "sass": "1.15.1",
    "stylelint": "9.8.0",
    "stylelint-config-recommended": "2.1.0"
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-router4-with-layouts": "^1.3.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  }
}
